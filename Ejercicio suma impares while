#include <iostream>
using namespace std;

int main() {
    int n;
    int i = 1;      
    int suma = 0;

    cout << "Ingrese un valor para n: ";
    cin >> n;

    cout << "NÃºmeros impares sumados: ";

    while (i <= n) {
        cout << i << " ";
        suma = suma + i;     
        i =i + 2;       
    }

    cout << endl;
    cout << "La suma de los impares hasta " << n << " es: " << suma << endl;

    return 0;
}
