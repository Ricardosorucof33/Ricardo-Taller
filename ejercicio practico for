#include <iostream>
using namespace std;

int main() {
    int i, n, suma = 0;
    do {
        cout << "Ingrese valor de n: ";
        cin >> n;
    } while (n<=0) ;

    for (i = 1; i <= n; i++) {
        if (i % 2 != 0)
            suma = suma + i;
    }

    cout << "La suma de los numeros impares es igual a " << suma;
    return 0;
}
