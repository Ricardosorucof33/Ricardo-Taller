#include <iostream>
using namespace std;
int main()
{
	int a, b, c,d, e, f; 
   cout << "Ingrese el primer numero: \n";
   cin >> a;
   cout << "Ingrese el segundo numero: \n";
   cin >> b;
   c = a + b;
   d = a - b;
   e = a * b;
   f = a / b;
   cout << "La suma es: " << c << endl;
   cout << "La resta es: " << d  <<endl;
   cout << "La multiplicacion es: " << e  <<endl;
   cout << "La division es: " << f << endl;
}
