#include <iostream> // Libreria para leer y escribir

using namespace std; //Que la entrada va por el teclado y la salida es el monitor 

int main()
{
cout<<"Bajo el cielo más puro de América" <<std::endl;
cout<<"Y en la tierra de Ñuflo de Chávez" <<std::endl;
std::cout<<"Libertad van trinando las aves" <<std::endl;
std::cout<<"De sureste ostentando el primor" <<std::endl;


    return 0;
}
